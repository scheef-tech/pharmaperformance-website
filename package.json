{
  "name": "medella-website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "bun run build && wrangler pages dev",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "deploy": "bun run build && wrangler pages deploy",
    "cf-typegen": "wrangler types && mv worker-configuration.d.ts src/",
    "db:gen": "drizzle-kit generate",
    "db:up": "wrangler d1 migrations apply medella",
    "db:studio": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:reset": "rm -rf .wrangler && bun run db:up",
    "prod:up": "wrangler d1 migrations apply medella --remote"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@inlang/paraglide-js": "1.11.3",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-cloudflare": "^4.7.4",
    "@sveltejs/enhanced-img": "^0.4.1",
    "@sveltejs/kit": "^2.8.5",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.28.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "5.1.11",
    "svelte-check": "^4.1.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.1",
    "wrangler": "^3.91.0"
  },
  "type": "module",
  "dependencies": {
    "@inlang/paraglide-sveltekit": "0.11.1",
    "@terrxo/unpic-svelte": "^0.0.57",
    "bits-ui": "^0.21.16",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "formsnap": "^1.0.1",
    "lucide-svelte": "^0.462.0",
    "resend": "4.0.0",
    "sveltekit-superforms": "^2.20.1",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "thumbhash": "^0.1.1",
    "zod": "^3.23.8"
  },
  "trustedDependencies": [
    "@sveltejs/kit",
    "esbuild",
    "sharp",
    "svelte-preprocess",
    "workerd"
  ]
}
